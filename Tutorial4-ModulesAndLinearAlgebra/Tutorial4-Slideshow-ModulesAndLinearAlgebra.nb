Cell[TextData[{
 "Sometimes when you\[CloseCurlyQuote]re building functions, you want to be \
sure that there is no overlap between variables defined ",
 StyleBox["globally. ",
  FontWeight->"Bold"],
 "\[LineSeparator]One way to ensure this is to use a ",
 StyleBox["Module.",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.780277183696945*^9, 
  3.780277267356798*^9}},ExpressionUUID->"7e8f5e15-c72d-46fe-93b0-\
1ab927b786d4"],

Cell[BoxData[
 RowBox[{"?", "Module"}]], "Input",
 CellChangeTimes->{{3.71543452207788*^9, 
  3.715434523595508*^9}},ExpressionUUID->"33a2eb05-4f53-4f50-a9d6-\
634736f8fac2"],

Cell[TextData[{
 "Modules are made up of three parts:\n\t",
 StyleBox["1. ",
  FontWeight->"Bold"],
 "A list declaring ",
 StyleBox["local variables",
  FontWeight->"Bold"],
 " in the Module\n\t",
 StyleBox["2. ",
  FontWeight->"Bold"],
 "Any intermediate calculations separated by ",
 StyleBox["semicolons ",
  FontWeight->"Bold"],
 "(",
 StyleBox[";",
  FontWeight->"Bold"],
 ")\n\t",
 StyleBox["3. ",
  FontWeight->"Bold"],
 "A final command which outputs some result."
}], "Text",
 CellChangeTimes->{{3.780278446586088*^9, 3.780278574967581*^9}, {
  3.7802786335501137`*^9, 
  3.780278643474937*^9}},ExpressionUUID->"6c72a940-7ea8-4318-b0de-\
b479e70b4949"],

Cell["Local variables only exist within the Module", "Text",
 CellChangeTimes->{{3.7802785968106413`*^9, 
  3.780278611036613*^9}},ExpressionUUID->"9a877da7-e985-44bc-b864-\
585b245142ee"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "x", "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x", "=", "6"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", "x", "]"}], "\[IndentingNewLine]", 
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", "x", "}"}], ",", 
   RowBox[{
    RowBox[{"x", "=", "5"}], ";", " ", 
    RowBox[{"Print", "[", "x", "]"}]}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", "x", "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Clear", "[", "x", "]"}]}], "Input",
 CellChangeTimes->{{3.7802786142390423`*^9, 3.7802787273080072`*^9}},
 CellLabel->"In[39]:=",ExpressionUUID->"50b45a6c-c754-4acb-8072-a0b5257d832c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", "example", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"myFunction", "[", "z0_", "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "localvar", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"localvar", "=", 
        RowBox[{"FindRoot", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Tan", "[", "z", "]"}], "\[Equal]", 
            RowBox[{"Sqrt", "[", 
             RowBox[{
              RowBox[{
               SuperscriptBox["z0", "2"], "/", 
               SuperscriptBox["z", "2"]}], "-", "1"}], "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"z", ",", 
            RowBox[{"\[Pi]", "/", "4"}]}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         SuperscriptBox["z0", "2"], "-", 
         SuperscriptBox["z", "2"]}], "/.", "localvar"}]}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"myFunction", "[", "z0", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"z0", ",", "0.8", ",", "6"}], "}"}]}], "]"}]}]}]], "Input",
