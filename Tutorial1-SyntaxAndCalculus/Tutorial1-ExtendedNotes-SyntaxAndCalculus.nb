Cell[TextData[{
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " is telling us it\[CloseCurlyQuote]s found a solution, but only under \
certain conditions of our parameters ",
 StyleBox["a",
  FontWeight->"Bold"],
 " and ",
 StyleBox["b",
  FontWeight->"Bold"],
 ". ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " is making sure our solution is not singular.\nIf we specify right away \
that ",
 StyleBox["a",
  FontWeight->"Bold"],
 " and ",
 StyleBox["b",
  FontWeight->"Bold"],
 " are real and greater than zero, then..."
}], "Text",
 CellChangeTimes->{{3.681838523918583*^9, 3.681838659074835*^9}, {
  3.681838716337429*^9, 
  3.681838720873949*^9}},ExpressionUUID->"a5c668dc-841f-46e6-8128-\
2f75252f2fb1"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Assuming", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"a", "\[Element]", "Reals"}], ",", 
     RowBox[{"b", "\[Element]", "Reals"}], ",", 
     RowBox[{"a", ">", "0"}], ",", 
     RowBox[{"b", ">", "0"}]}], "}"}], ",", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"x", "^", "2"}]}], ")"}], "^", "2"}], 
        RowBox[{"Sqrt", "[", 
         RowBox[{"b", "+", 
          RowBox[{"a", "*", 
           RowBox[{"x", "^", "2"}]}]}], "]"}]}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.681838664902397*^9, 
  3.68183871218108*^9}},ExpressionUUID->"d19bcecc-fca3-413a-be28-\
800f50abbc92"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{
    SqrtBox["a"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "a"}], "+", "b"}], ")"}]}], "+", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "2"}], " ", "a"}], "+", "b"}], ")"}], " ", 
    SqrtBox[
     RowBox[{
      RowBox[{"-", "a"}], "+", "b"}]], " ", 
    RowBox[{"ArcSec", "[", 
     SqrtBox[
      FractionBox["b", "a"]], "]"}]}]}], 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{"a", "-", "b"}], ")"}], "2"]]], "Output",
 CellChangeTimes->{{3.681838700608241*^9, 
  3.6818387256583967`*^9}},ExpressionUUID->"0bbe2e86-f558-44e3-9179-\
040f0b6f6c17"]
}, Open  ]],

