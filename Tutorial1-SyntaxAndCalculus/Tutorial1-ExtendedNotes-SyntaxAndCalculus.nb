     RowBox[{"Gamma", "[", "x", "]"}], "+", 
     RowBox[{"BesselJ", "[", 
      RowBox[{"1", ",", 
       RowBox[{"x", " ", "y"}]}], "]"}]}], ",", "x", ",", 
    RowBox[{"{", 
     RowBox[{"y", ",", "2"}], "}"}]}], "]"}], "/.", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"x", "\[Rule]", "2"}], ",", 
    RowBox[{"y", "\[Rule]", "2"}]}], "}"}]}]], "Input",
