           RowBox[{"(", 
            RowBox[{
             RowBox[{
              SqrtBox[
               FractionBox["a", "b"]], " ", "b", " ", "\[Pi]"}], "+", 
             RowBox[{"2", " ", "a", " ", 
              RowBox[{"(", 
               RowBox[{
                SqrtBox[
                 RowBox[{"1", "-", 
                  FractionBox["a", "b"]}]], "-", 
                RowBox[{
                 SqrtBox[
                  FractionBox["a", "b"]], " ", "\[Pi]"}]}], ")"}]}]}], 
            ")"}]}], "+", 
          RowBox[{"2", " ", 
